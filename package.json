{
  "name": "authvisage-sdk",
  "version": "0.4.0",
  "description": "authvisage client sdk",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --watch",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "prepare": "husky install",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "tsup": "^8.5.0",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.10",
    "eslint": "^9.22.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^15.4.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeyadAjamy/authvisage-sdk.git"
  },
  "keywords": [
    "authvisage",
    "face",
    "authentication",
    "face-auth",
    "authentication",
    "supabase",
    "sdk",
    "face id",
    "biometric"
  ],
  "author": "Zeyad Alajamy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeyadAjamy/authvisage-sdk/issues"
  },
  "homepage": "https://github.com/zeyadAjamy/authvisage-sdk#readme",
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
